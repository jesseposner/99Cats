<tr>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <%if current_user.cats.include?(cat)%>
    <% if req.pending? %>
        <%= button_to "Approve", approve_cat_rental_request_url(req) %>
        <%= button_to "Deny", deny_cat_rental_request_url(req) %>
    <% else %>
      <%= req.status %>
    <% end %>
    <%end%>
  </td>
  <td><%= User.find(req.user_id).user_name %></td>
</tr>
